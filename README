regenerate mysqldump.sql:
mysqldump -u root --default-character-set=latin1 --add-drop-table --no-data moneyflow > /mnt/www/sites/olli.homeip.net/htdocs/moneyflow/mysqldump.sql
mysqldump -u root --default-character-set=latin1 --tables moneyflow currencies text templates settings |grep INSERT >> /mnt/www/sites/olli.homeip.net/htdocs/moneyflow/mysqldump.sql
